
.page {
	height: 100vh;
	width:100vw;
}
.cu-form-group{
	position: relative;
	border-top:0px solid #eee;
}
.cu-form-x{
	width:calc(100% - 120rpx);
	height: 0.5rpx;
	background-color: #EEEEEE;
	position: absolute;
	bottom:0.5rpx;
	margin-left:50rpx;
}
.hui {
	color: #E1E1E1;
}
.cu-form-group+.cu-form-group {
	border-top:0px solid #eee;
}

